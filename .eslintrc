{
	"env": {
		"es6": true,
		"node": true,
		"browser": true
	},
	"rules": {
		"indent": [
			1,
			"tab"
		],
		"quotes": [
			1,
			"double"
		],
		"linebreak-style": [
			1,
			"unix"
		],
		"semi": [
			2,
			"always"
		]
	},
	"ecmaFeatures": {
		"modules": true,
		"jsx": true
	},
	"plugins": [
		"react"
	]
}
